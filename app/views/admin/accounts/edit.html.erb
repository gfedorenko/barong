<div class="container">
  <h2>Edit</h2>
  <%= form_for([:admin, @account], method: :put) do |f| %>

    <div class="form-group">
      <%= f.label :role %><br />
      <%= f.text_field :role, autocomplete: 'role' %>
    </div>

    <div class="actions">
      <%= f.submit 'Update', class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>